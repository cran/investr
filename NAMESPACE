
export(
  calibrate,
  invest,
  plotFit
)    

## Methods
S3method(calibrate, default)    
S3method(calibrate, formula)
S3method(calibrate, lm)  
S3method(invest, lm)    
S3method(invest, nls) 
S3method(invest, lme)  # new (experimental)  
S3method(plotFit, lm)    
S3method(plotFit, nls) 
S3method(print, calibrate)
## Recommended packages
importFrom(nlme, fixef, asOneFormula, getVarCov)



